<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@drawable/back"
    >

    <ImageView android:id="@+id/topMenu"
        android:src="@drawable/menu0"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="70dip"
        android:scaleX="0.6"
        android:scaleY="0.6"
        />
    
    <ImageView android:id="@+id/botMenu"
        android:src="@drawable/menu10"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="170dip"
        android:scaleX="0.6"
        android:scaleY="0.6"
        />
    
        <ImageView android:id="@+id/alertMenu"
        android:src="@drawable/menualertfalse"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="170dip"
        android:scaleX="0.6"
        android:scaleY="0.6"
        />
        
      <RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:id="@+id/normal"
    >       
	    <ProgressBar android:id="@+id/anim_front"

	        android:indeterminateDrawable="@drawable/front_anim"
	        android:layout_gravity="center"
	        android:layout_marginTop="64dip"
	        android:layout_marginLeft="9dip"
			android:scaleX="0.6"
			android:scaleY="0.6"
			android:layout_height="150dip"
            android:layout_width="200dip"
			
	        />
    </RelativeLayout>
    

    
    <ImageView android:id="@+id/buttonLeft"
        android:layout_height="50dip"
        android:layout_width="63dip"
        android:layout_marginTop="215dip"
        android:layout_marginLeft="20dip"
        android:src="@drawable/button"
        />
       
        <ImageView android:id="@+id/buttonMid"
        android:layout_height="50dip"
        android:layout_width="63dip"
        android:layout_marginTop="225dip"
        android:layout_marginLeft="75dip"
         android:src="@drawable/button"
        />
        
        <ImageView android:id="@+id/buttonRight"
        android:layout_height="50dip"
        android:layout_width="63dip"
        android:layout_marginTop="215dip"
        android:layout_marginLeft="130dip"
         android:src="@drawable/button"
     	
        
        />
        
    <TextView
        android:id="@+id/update"
        style="@android:style/TextAppearance.Medium"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:gravity="center_horizontal|center_vertical"
        android:layout_margin="4dip"
        android:text="Static poo" 
        android:visibility="invisible" >
    </TextView>
    
    <RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:id="@+id/blank"
    android:visibility="invisible"
    >       
    <ImageView
        android:id="@+id/eatMenu"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:src="@drawable/eatmenu00"
        android:layout_gravity="center"
        android:layout_marginTop="66dip"
        android:layout_marginLeft="10dip"
		android:scaleX="0.4"
		android:scaleY="0.4" />       
</RelativeLayout>
    
        <RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:id="@+id/anim_view"
    android:visibility="invisible"
    >       
    <ImageView
        android:id="@+id/anim"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:src="@drawable/eatmenu00"
        android:layout_gravity="center"
        android:layout_marginTop="64dip"
        android:layout_marginLeft="9dip"
		android:scaleX="0.6"
		android:scaleY="0.6" />       
</RelativeLayout>

</RelativeLayout>